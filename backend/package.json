{
  "name": "backend",
  "version": "1.0.0",
  "description": "NodeJs server boilerplate template with typescript",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "build": "tsc",
    "lint": "eslint '**/*.ts' --quiet",
    "prettier:check": "prettier --check **/*.ts",
    "prettier:write": "prettier --write **/*.ts",
    "test": "jest"
  },
  "keywords": [
    "Backend",
    "NodeJs",
    "Server",
    "Typescript"
  ],
  "author": "dhelarius",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/pg": "^8.6.6",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pg": "^8.9.0"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm",
    "pnpm": ">=7.17.1"
  }
}
